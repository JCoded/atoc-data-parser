{
    "name": "jcoded/atoc-data-parser",
    "description": "A parser for the ATOC data files.",
    "keywords": [
        "ATOC", 
        "Association of Train Operating Companies", 
        "uk rail", 
        "uk train",
        "train data parser"
    ],
    "license": "MIT",
    "type": "package",
    "support": {
        "issues": "https://github.com/JCoded/atoc-data-parser/issues"
    },
    "authors": [{
        "name": "James Smith",
        "homepage": "https://github.com/JCoded",
        "role": "Developer"
    }],
    "require": {
    "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/php-code-coverage": "~2.0",
        "league/flysystem": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "JCoded\\AtocDataParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JCoded\\AtocDataParser\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
